<?xml version="1.0"?>
<package>
  <name>udom_geometric_transformation</name>
  <version>0.1.0</version>
  <description>
    This package contains nodes to perform geometric transformations such as
    transforming a wrench from one frame of reference to another.
  </description>
  <license>GPLv3</license>

  <maintainer email="jose.sanchez.loza@gmail.com">Jose Sanchez</maintainer>
  <author email="jose.sanchez.loza@gmail.com">Jose Sanchez</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>
  <build_depend>roslint</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>tf2_ros</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>udom_common_msgs</build_depend>
  <build_depend>udom_modeling_msgs</build_depend>
  <build_depend>udom_control_msgs</build_depend>

  <run_depend>rospy</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>tf2_ros</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>tf2_geometry_msgs</run_depend>
  <run_depend>dynamic_reconfigure</run_depend>
  <run_depend>udom_common_msgs</run_depend>
  <run_depend>udom_modeling_msgs</run_depend>
  <run_depend>udom_control_msgs</run_depend>

  <test_depend>roslaunch</test_depend>
  <test_depend>rostest</test_depend>

</package>
